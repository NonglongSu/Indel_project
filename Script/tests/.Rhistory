readFile = function(inD, pat){
Files= list.files(inD,pattern=pat,full.names=T)
index = as.numeric(str_extract(basename(Files),'[^.]+')) #rectify the order
Files = Files[order(index)]
n    = length(Files)
res  = list(Files,n)
return(res)
}
